-- Scripts:

local function TZJUDFG_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	local Home = script.Parent.Side.Home
	local Main = script.Parent.Side.Main
	
	local MainBoard = script.Parent
	local HomeFrame = script.Parent.Pages.HomeFrame
	local MainFrame = script.Parent.Pages.MainFrame
	
	Home.MouseButton1Click:Connect(function()
		HomeFrame.Visible = true
		MainFrame.Visible = false
	end)
	Main.MouseButton1Click:Connect(function()
		HomeFrame.Visible = false
		MainFrame.Visible = true
	end)
	
	local gui = game.Players.LocalPlayer.PlayerGui:WaitForChild("Virus_PrisonLife")
	local input = game:GetService("UserInputService")
	
	local function toggleGui()
		gui.Enabled = not gui.Enabled
	end
	
	input.InputBegan:Connect(function(key)
		if key.KeyCode == Enum.KeyCode.RightShift then
			toggleGui()
		end
	end)
	
end
coroutine.wrap(TZJUDFG_fake_script)()
local function QXQT_fake_script() -- FpsCounterLabel.LocalScript 
	local script = Instance.new('LocalScript', FpsCounterLabel)

	local lastUpdate = tick()
	local frameCount = 0
	local label = script.Parent
	
	local function updateCounter()
		local now = tick()
		frameCount = frameCount + 1
	
		if now - lastUpdate >= 1 then
			local fps = math.floor(frameCount / (now - lastUpdate))
			label.Text = "FPS: " .. tostring(fps)
			frameCount = 0
			lastUpdate = now
		end
	end
	
	game:GetService("RunService").RenderStepped:Connect(updateCounter)
end
coroutine.wrap(QXQT_fake_script)()
local function CHEEH_fake_script() -- RegionLabel.LocalScript 
	local script = Instance.new('LocalScript', RegionLabel)

	local label = script.Parent
	
	local function updateRegionLabel()
		local player = game.Players.LocalPlayer
		local character = player.Character
	
		if character then
			local region = game:GetService("Workspace"):FindFirstChildWhichIsA("Region3", true)
	
			if region then
				local min, max = region:GetBoundingBox()
				local regionName = tostring(region.Name)
	
				label.Text = "Region: " .. regionName
			end
		end
	end
	
	-- connect to the HumanoidRootPart changed event to update the region label
	game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
		character:WaitForChild("HumanoidRootPart").Changed:Connect(updateRegionLabel)
	end)
	
	updateRegionLabel()
end
coroutine.wrap(CHEEH_fake_script)()
